#include <stdio.h>

int main(){
    float arr[100], total;
    printf("Enter total number of element : ");
    scanf("%f", &total);

    for(int i=0; i<total; i++){
        printf("Enter Number %d : ",i+1);
        scanf("%f", &arr[100]);
    }

    for(int i = 0; i<total; i++){
       arr[100] < arr[i];
    }

    printf("Largest element = %f", arr[100]);
    
    return 0;
}
